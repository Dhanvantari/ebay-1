</div>
<!-- my-ebay Page content here -->

<div id="my-ebay" class="my-ebay">
    <div class="me-rtm topRtm">
        <!-- FOR US/UK/DE -->
        <div id="rtm_html_644" style="min-height: 30px;"></div>
        <!-- FOR CA/AUS -->
    </div>
    <div id="top-nav">
        <div class="tabbable">
            <ul class="nav nav-tabs-p" role="navigation">
                <li class="tab">
                    <a href="/addcategory">Add Category</a>
                </li>
                <li class="tab active">
                    <a href="/delcategory">Delete Category</a>
                </li>
                <li class="tab">
                    <a href="/list/sellers">Sellers</a>
                </li>
                <li class="tab">
                    <a href="/ListCustomers">Customers</a>
                </li>
                <li class="tab">
                    <a href="/ListProducts">Products</a>
                </li>
                <li class="tab">
                    <a href="/displayAuction">Display Bids</a>
                </li>
            </ul>
        </div>
        <div class="tabbable-bottom"></div>
    </div>
     <div id="domain-nav">
        <div class="menu-list" role="navigation">
            <p class="menu-head selected">Delete Category</p>
        </div>
    </div>
    <div id="main-content">
    <%console.log("allCategories---",allCategories);%>
        <form action="/delcategory" method="post" class="form-horizontal" style="margin-left: 40px;">
			
			<div id="form-group">
                                     <select title="Categories" class="gh-sb gh-sprRetina" size="1" id="gh-cat" name="category">
                                        <option selected="selected" value="all">All Categories</option>
                                       <%for(var cat =0;cat<allCategories.length;cat++){%>
                                       <option value="<%=allCategories[cat].category%>"><%=allCategories[cat].category%></option>
                                       <%}%>
                                    </select>
                                    
                                </div>
			
            <div class="form-group">
                <label class="control-label col-xs-2">Delete Category : </label>
                <div class="col-xs-6">
                    <input class="form-control" type="text" name="Category" placeholder = "Electronics,Toys etc.">
                </div>
            </div>  
            <div class="form-group">
                <div class="col-sm-offset-2 col-sm-10">
                    <button type="submit" class="btn btn-prim btn-s">Remove</button>
                </div>
            </div>  
            
            </form>
            </div>
            </div>
